{
    "name": "Markdown URL",
    "version": "1.0",
    "manifest_version": 3,
    "description": "This will help you to markdown url",
    "background": {
        "service_worker": "bg.js"
    },
    "action": {
        "default_popup": "popup.html"
    }, 
    "permissions": [
        "storage",
        "activeTab", 
        "tabs", 
        "clipboardRead"
    ]
}